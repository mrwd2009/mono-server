{
  "name": "simple-service",
  "version": "1.0.0",
  "description": "simple service",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build": "rm -rf ./dist/* && tsc -p tsconfig.build.json",
    "prettier": "prettier --write --ignore-unknown ./src",
    "pretty-quick": "pretty-quick --staged",
    "format": "npm run prettier",
    "dev": "nodemon src/index.ts",
    "dev-cluster": "nodemon src/cluster.ts",
    "dev-log-server": "nodemon src/log-server.ts",
    "dev-queue": "nodemon src/queue.ts",
    "dev-queue-dashboard": "nodemon src/queue-dashboard.ts",
    "dev-schedule": "nodemon src/schedule.ts",
    "build-image:prod": "yarn build && docker build -t app-service:${APP_VERSION:-1.0.0} . ",
    "image-cli:prod": "docker run -it --rm app-service:${APP_VERSION:-1.0.0} sh",
    "run-container:prod": "docker run --name ${APP_CONAINER_NAME:-app-service-container} --env-file .env -d  -p ${APP_PORT:-5700}:5700  app-service:${APP_VERSION:-1.0.0}",
    "remove-container:prod": "docker rm -f ${APP_CONAINER_NAME:-app-service-container}"
  },
  "author": "wudi.link.me@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@tsconfig/node14": "^1.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.15.8",
    "@types/cookie": "^0.4.0",
    "@types/ioredis": "^4.28.8",
    "@types/jest": "^26.0.23",
    "@types/koa": "^2.13.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-passport": "^4.0.2",
    "@types/koa__cors": "^3.0.3",
    "@types/koa__multer": "^2.0.4",
    "@types/koa__router": "^8.0.4",
    "@types/lodash": "^4.14.170",
    "@types/multer": "^1.4.7",
    "@types/node": "^15.6.1",
    "@types/node-schedule": "^1.3.2",
    "@types/passport-jwt": "^3.0.5",
    "@types/triple-beam": "^1.3.2",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-jest": "^27.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.0.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.2.4"
